<div id="<%= field %>" class="subFuncContent panel panel-default" style="display: <% if(show){ %>block<% }else{ %> none <% } %>" >
    <script src="/js/admin/testArrange/examPlan.js"></script>
    <div class="panel-heading">
        <h3 class="panel-title"><%= name %></h3>
    </div>
    <div class="panel-body">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-8">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-5 examSites">
                                <h4>考点管理</h4>
                                <hr>
                                <table></table>
                                <br>
                                <button type="button" class="btn btn-primary btn-lg btn-block addSite">添加考点</button>
                            </div>
                            <div class="col-md-7 subjects">
                                <h4>科目管理</h4>
                                <hr>
                                <table></table>
                                <br>
                                <button type="button" class="btn btn-primary btn-lg btn-block addSubject">添加科目</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 limitRules">
                    <h4>人数限制规则管理</h4>
                    <hr>
                    <table></table>
                    <br>
                    <button type="button" class="btn btn-primary btn-lg btn-block addSubject">添加人数限制规则</button>
                </div>
            </div>
        </div>
    </div>
</div>