<div id="<%= field %>" class="subFuncContent panel panel-default" style="display: <% if(show){ %>block<% }else{ %> none <% } %>" >
    <script src="/js/admin/configs/dbBackup.js"></script>
    <div class="panel-heading">
        <h3 class="panel-title"><%= name %></h3>
    </div>
    <div class="panel-body">
        <div class="container-fluid">
            <div class="row">
                <!-- 备份计划 -->
                <div class="col-lg-4 col-sm-6">
                    <form action="/admin/configs/dbBackup" method="post">
                        <div class="page-header">
                            <h4>备份计划</h4>
                        </div>
                        <div class="form-group message">

                        </div>
                        <div class="form-group">
                            <label>备份时间(crontab表达式)</label>
                            <input name="schedule" class="form-control" type="text" placeholder="* * * * *">
                        </div>
                        <div class="form-group">
                            <input type="submit" class="btn btn-primary" value="提交">
                        </div>
                        <div class="form-group">
                            <b>注意:对备份计划的任何修改将在服务器下次启动时生效</b>
                        </div>
                    </form>
                    <div id="crontabGenerator">

                        <div class="page-header">
                            <h4>crontab表达式生成器</h4>
                        </div>
                        <ul class="nav nav-pills">

                            <li role="presentation" class="active" segment="minutes"><a href="javascript:void(0);">分</a></li>
                            <li role="presentation" segment="hours"><a href="javascript:void(0);">时</a></li>
                            <li role="presentation" segment="days"><a href="javascript:void(0);">天</a></li>
                            <li role="presentation" segment="mouths"><a href="javascript:void(0);">月</a></li>
                            <li role="presentation" segment="weeks"><a href="javascript:void(0);">星期</a></li>
                        </ul>
                        <div class="minutes content" segment="minutes" style="display:block;">
                            <div class="form-inline form-group" mode="each">
                                <input type="radio" name="minuteType" checked>每分钟
                            </div>
                            <div class="form-inline form-group" mode="interval">
                                <input type="radio" name="minuteType">
                                每隔 <%- include('../rangeSelect',{min:1,max:58}) %> 分钟
                            </div>
                            <div class="form-inline form-group" mode="stable">
                                <input type="radio" name="minuteType">
                                固定 <%- include('../rangeSelect',{min:0,max:59}) %> 分
                            </div>
                            <div class="form-inline form-group" mode="contain">
                                <input type="radio" name="minuteType">
                                从 <%- include('../rangeSelect',{min:0,max:59}) %> 分
                                至 <%- include('../rangeSelect',{min:0,max:59}) %> 分
                            </div>
                        </div>
                        <div class="hours content" segment="hours">
                            <div class="form-inline form-group" mode="each" >
                                <input type="radio" name="hourType" checked>每小时
                            </div>
                            <div class="form-inline form-group" mode="interval">
                                <input type="radio" name="hourType">
                                每隔 <%- include('../rangeSelect',{min:1,max:23}) %> 小时
                            </div>
                            <div class="form-inline form-group" mode="stable">
                                <input type="radio" name="hourType">
                                固定 <%- include('../rangeSelect',{min:0,max:23}) %> 时
                            </div>
                            <div class="form-inline form-group" mode="contain">
                                <input type="radio" name="hourType">
                                从 <%- include('../rangeSelect',{min:0,max:23}) %> 时
                                至 <%- include('../rangeSelect',{min:0,max:23}) %> 时
                            </div>
                        </div>
                        <div class="days content" segment="days">
                            <div class="form-inline form-group" mode="each">
                                <input type="radio" name="dayType" checked>每日
                            </div>
                            <div class="form-inline form-group" mode="interval">
                                <input type="radio" name="dayType">
                                每隔 <%- include('../rangeSelect',{min:1,max:29}) %> 日
                            </div>
                            <div class="form-inline form-group" mode="stable">
                                <input type="radio" name="dayType">
                                固定 <%- include('../rangeSelect',{min:1,max:31}) %> 日
                            </div>
                            <div class="form-inline form-group" mode="contain">
                                <input type="radio" name="dayType">
                                从 <%- include('../rangeSelect',{min:1,max:31}) %> 日
                                至 <%- include('../rangeSelect',{min:1,max:31}) %> 日
                            </div>
                            <div class="form-inline form-group" mode="none">
                                <input type="radio" name="dayType">不指定
                            </div>
                        </div>
                        <div class="mouths content" segment="mouths">
                            <div class="form-inline form-group" mode="each">
                                <input type="radio" name="mouthType" checked>每月
                            </div>
                            <div class="form-inline form-group" mode="interval">
                                <input type="radio" name="mouthType">
                                每隔 <%- include('../rangeSelect',{min:1,max:10}) %> 月
                            </div>
                            <div class="form-inline form-group" mode="stable">
                                <input type="radio" name="mouthType">
                                固定 <%- include('../rangeSelect',{min:1,max:12}) %> 月
                            </div>
                            <div class="form-inline form-group" mode="contain">
                                <input type="radio" name="mouthType">
                                从 <%- include('../rangeSelect',{min:1,max:12}) %> 月
                                至 <%- include('../rangeSelect',{min:1,max:12}) %> 月
                            </div>
                        </div>
                        <div class="weeks content" segment="weeks">
                            <div class="form-inline form-group" mode="each">
                                <input type="radio" name="weekType" checked>每天
                            </div>
                            <div class="form-inline form-group" mode="stable">
                                <input type="radio" name="weekType">
                                每个星期 <%- include('../rangeSelect',{min:0,max:7}) %>
                            </div>
                            <div class="form-inline form-group" mode="contain">
                                <input type="radio" name="weekType">
                                从星期 <%- include('../rangeSelect',{min:0,max:7}) %>
                                至星期 <%- include('../rangeSelect',{min:0,max:7}) %>
                            </div>
                            <div class="form-inline form-group" mode="none">
                                <input type="radio" name="weekType">不指定
                            </div>
                        </div>
                        <p>
                            <div class="form-inline form-group">
                                <button class="btn btn-primary calculate">生成表达式</button>
                                <input class="form-control result" type="text" disabled>
                            </div>
                        </p>
                    </div><!-- crontabGenerator end -->
                </div>
                <!-- 备份管理 -->
                <div class="col-lg-8 col-sm-6">
                    <div class="page-header">
                        <h4>备份管理</h4>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>